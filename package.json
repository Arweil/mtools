{
  "name": "mtools",
  "version": "1.0.0",
  "description": "Milk's toolkit",
  "keywords": [
    "toolkit"
  ],
  "license": "MIT",
  "author": "Milk",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build",
    "changelog": "lerna-changelog",
    "prepare": "husky install && lerna bootstrap",
    "site": "dumi dev",
    "site:build": "dumi build",
    "test": "lerna run test"
  },
  "lint-staged": {
    "*.{less,json}": [
      "prettier --write",
      "git add"
    ],
    "*.js?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ],
    "*.less": [
      "stylelint --syntax less"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/jest": "^26.0.23",
    "@umijs/fabric": "^2.14.1",
    "commitlint": "^17.6.3",
    "dumi": "^1.1.18",
    "dumi-theme-default": "^1.1.1",
    "eslint": "^7.32.0",
    "husky": "^8.0.3",
    "jest": "^27.0.4",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^2.56.2",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.3"
  }
}
